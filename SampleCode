i=1
for (i in 1:66) {
  test=popGrowthByDecade
  x=i+6
  r=x+6
  test=test[x:r,]
  t=lm(Row.1~Label, data=test)
  summary(t)
  df2$GrowthSlope[i] = t$coefficients[[2]]
}

